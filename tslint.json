{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "prettier": true,
    "no-console": {
      "severity": "warning"
    },
    "interface-name": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}